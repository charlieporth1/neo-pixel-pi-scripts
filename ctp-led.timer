[Unit]
Description=Logs some system statistics to the systemd journal
Requires=ctp-led.service

[Timer]
Unit=ctp-led.service
OnCalendar=Mon..Fri *-*-* 17..23:00/5:00
OnCalendar=Sun,Sat *-*-* 06..23:00/5:00
Persistent=true

[Install]
WantedBy=timers.target
